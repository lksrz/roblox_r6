    elseif phase == "Live" or phase == "InProgress" then
        phaseLabel.Text = "ðŸŽ® LIVE"
        phaseLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
    elseif phase == "Intermission" then
        phaseLabel.Text = "â¸ BREAK"
