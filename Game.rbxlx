    elseif phase == "Prep" then
        phaseLabel.Text = "â± GET READY"
        phaseLabel.TextColor3 = Color3.fromRGB(255, 255, 100)
    elseif phase == "End" then
        phaseLabel.Text = "ðŸ† ROUND OVER"
